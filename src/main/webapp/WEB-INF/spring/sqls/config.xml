<!--config 태그 추가 -->

<configuration>
	<typeAliases>
		<!-- <typeAlias type = "com.yee.carebank.model.dto.BoardDto" alias = "boardDto"/> 
			형식으로 추가해주면됌 -->
		<typeAlias type="com.yee.carebank.model.dto.MealDto"
			alias="mealDto" />
		<typeAlias type="com.yee.carebank.model.dto.FoodDto"
			alias="foodDto" />
		<typeAlias type="com.yee.carebank.model.dto.SuppleDto"
			alias="suppleDto" />
		<typeAlias type="com.yee.carebank.model.dto.CommentDto"
			alias="commentDto" />
    	<typeAlias type = "com.yee.carebank.model.dto.FaqDto" alias = "faqDto"/>
  		<typeAlias type = "com.yee.carebank.model.dto.UserDto" alias = "userDto"/>
    	<typeAlias type = "com.yee.carebank.model.dto.ExerciseDto" alias = "ExerciseDto"/>
  		<typeAlias type = "com.yee.carebank.model.dto.MeditationDto" alias = "MeditationDto"/>
