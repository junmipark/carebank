		<typeAlias type = "com.yee.carebank.model.dto.ScheduleDto" alias="scheduleDto"/>
		<typeAlias type = "com.yee.carebank.model.dto.MoodDto" alias = "moodDto"/>
		<typeAlias type = "com.yee.carebank.model.dto.PillsDto" alias = "pillsDto" />
		<typeAlias type="com.yee.carebank.model.dto.MyMealDto" alias="myMealDto" />
	</typeAliases>

	<mappers>
		<!-- <mapper resource = "/mybatis/board-mapper.xml"/> 형식으로 매퍼 위치 추가 작성 -->
		<mapper resource="/mybatis/meal-mapper.xml" />
		<mapper resource="/mybatis/supple-mapper.xml" />
		<mapper resource="/mybatis/comment-mapper.xml" />		
    	<mapper resource = "/mybatis/faq-mapper.xml"/>
		<mapper resource = "/mybatis/user-mapper.xml"/>
    	<mapper resource = "/mybatis/exercise-mapper.xml"/>
  		<mapper resource = "/mybatis/meditation-mapper.xml"/>
  		<mapper resource = "/mybatis/myexercise-mapper.xml"/>
	    <mapper resource = "/mybatis/schedule-mapper.xml"/>
  		<mapper resource = "/mybatis/mood-mapper.xml"/>
  		<mapper resource = "/mybatis/pills-mapper.xml"/>
  		<mapper resource = "/mybatis/mymeal-mapper.xml"/>
	</mappers>

</configuration>
